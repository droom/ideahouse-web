- var authorised = true
- var page = 'product'

html
  include partials/_head
  body
    include partials/_brand
    .wrap
      include partials/_nav

      .main
        .ui.huge.breadcrumb
          a.section(href="search-results.html") Advanced Search
          i.right.angle.icon.divider
          .section Umbra 21 Inches Tri Fold Open / Close Automatic Umbrella

        .ui.two.column.grid.cycle
          .row
            .column
              button.ui.labeled.icon.tiny.cycle.button
                i.left.arrow.icon
                p Previous item
            .column.right.aligned
              button.ui.right.labeled.icon.tiny.cycle.button
                i.right.arrow.icon
                p Next item

        hr

        .product-wrap
          img(src="img/product.png", class="product-hero")
          .ui.labeled.icon.black.button.download Download
            i.cloud.download.icon

        .ui.two.item.menu
          a.item.active(class="btn-details") Product Details
